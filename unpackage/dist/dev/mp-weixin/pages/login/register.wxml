<view><view class="top">注册袋鼠宝</view><input class="input" type="number" placeholder="输入手机号" placeholder-style="color:#C2C2C2;font-size:32rpx" data-event-opts="{{[['input',[['__set_model',['','uname','$event',[]]]]]]}}" value="{{uname}}" bindinput="__e"/><view class="get_code"><input class="code" type="number" placeholder="验证码" placeholder-style="color:#C2C2C2;font-size:32rpx" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['get_code',['$event']]]]]}}" hidden="{{!(show)}}" class="text" bindtap="__e">点击获取</view><text hidden="{{!(!show)}}" class="text_r">{{"重新发送("+count+"s)"}}</text></view><input class="input" type="password" placeholder="请输入密码" placeholder-style="color:#C2C2C2;font-size:32rpx" data-event-opts="{{[['input',[['__set_model',['','upwd','$event',[]]]]]]}}" value="{{upwd}}" bindinput="__e"/><input class="input" type="password" placeholder="请确认密码" placeholder-style="color:#C2C2C2;font-size:32rpx" data-event-opts="{{[['input',[['__set_model',['','repassword','$event',[]]]]]]}}" value="{{repassword}}" bindinput="__e"/><input class="input" type="number" placeholder="邀请人手机号" placeholder-style="color:#C2C2C2;font-size:32rpx" data-event-opts="{{[['input',[['__set_model',['','EXphoto','$event',[]]]]]]}}" value="{{EXphoto}}" bindinput="__e"/><view class="agreement"><view data-event-opts="{{[['tap',[['read_xieyi',['$event']]]]]}}" class="dian" bindtap="__e"><view hidden="{{!(read)}}" class="dian_s"></view></view><view data-event-opts="{{[['tap',[['agreement',['$event']]]]]}}" class="font" bindtap="__e">注册协议</view></view><block wx:if="{{!disabled}}"><button data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="btn" bindtap="__e">注册</button></block><block wx:if="{{disabled}}"><button class="btn" disabled="disabled" data-event-opts="{{[['tap',[['register',['$event']]]]]}}" bindtap="__e">注册</button></block></view>